{
  "extends": [
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
    }
  },
  "rules": {
    "object-curly-spacing": [
      "warn",
      "always"
    ],
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": "off",
    "max-len": [
      "warn",
      {
        "code": 120,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreComments": true
      }
    ],
    "camelcase": "off",
    "react/prop-types": [
      0
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
